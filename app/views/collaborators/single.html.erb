



                <center><h1>Collaborators</h1></center>
                <nav class="navbar navbar-static-top" role="navigation">

                    <div class="btn-group">
                        <button type="button" class="btn btn-default"><i class="fa fa-reorder"></i></button>
                        <button type="button" class="btn btn-default"><i class="fa fa-dot-circle-o"></i></button>
                    </div>

                    <div class="btn-group">
                        <a href="#" class="btn btn-block btn-AddCollaborator btn-bitbucket"><i class="fa fa-plus"></i>Add new Collaborator </a>
                   
                    </div>
                    <div class="btn-group">
                      
                          <button class="btn btn-lg btn-danger" data-toggle="modal" data-target="#smallModal"><i class="fa fa-plus"></i>Advanced Search</button> 
                    </div>

                </nav>

                <table>
  
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @collaboinfos.each do |f| %>
      <tr>
        <td><%= f.picture %></td>
      </tr>
    <% end %>

    <% @collabo.each do |f| %>
      <tr>
        <td><%= f.names %></td>
        <td><%= f.last_names %></td>
      </tr>
    <% end %>

    <% @collaboworks.each do |f| %>
      <tr>
        <td><%= f.account %></td>
      </tr>
    <% end %>

    <% @collabocontacts.each do |f| %>
      <tr>
        <td><%= f.email %></td>
      </tr>
    <% end %>



  </tbody>
</table>

